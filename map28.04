         body {
            font-family: 'Outfit', sans-serif !important;
        }
        /* Map Container */
        #map { 
            height: 650px; 
            width: 100%; 
        }

        /* Remove border-radius & margin from the popup */
        .leaflet-popup-content-wrapper, 
        .leaflet-popup-content {
            border-radius: 0 !important;
            margin: 0 !important;
        }

        /* Increased width of popup */
        .leaflet-popup-content {
            width: 420px !important;
        }

        /* Property Type Label */
        .popup-type { 
            background: #222; 
            color: #fff; 
            text-transform: uppercase; 
            font-size: 12px; 
            font-weight: bold; 
            display: flex; 
            justify-content: space-between;
            align-items: center;
            padding: 8px 12px;
            font-family: "Outfit", sans-serif !important;
        }

        /* Close Button */
         .leaflet-container a.leaflet-popup-close-button {
            cursor: pointer;
            font-size: 16px;
            font-weight: bold;
            color: #fff;
            background: transparent;
            border: none;
        }
         .leaflet-container a.leaflet-popup-close-button:hover {
          
            color: #f2f2f2;
            
        }

        /* Popup Content */
        .popup-content { 
            display: flex; 
            background: #fff; 
            padding: 12px;
            align-items: center;
        }

        /* Property Image */
        .popup-image { 
            width: 100px; 
            height: 70px; 
            object-fit: cover; 
            border: 1px solid #ddd;
        }

        /* Property Title */
        .popup-title { 
            font-size: 14px; 
            font-weight: bold; 
            flex: 1; 
            padding-left: 12px; 
            font-family: "Outfit", sans-serif !important;
        }

        /* Additional Details Section */
        .popup-details {
            padding: 12px;
            background: #f9f9f9;
            font-size: 13px;
            font-family: "Outfit", sans-serif !important;
        }

        .popup-details .subtitle {
            font-weight: bold;
            margin-bottom: 6px;
            font-family: "Outfit", sans-serif !important;
        }

        .popup-details ul {
            list-style-type: disc;
            padding-left: 16px;
            margin: 0;
            font-family: "Outfit", sans-serif !important;
        }

       /* Contact Section */
       /* Contact Section */
.popup-contact { 
    background: #f1f1f1; 
    padding: 12px;
    display: grid; /* Use grid for layout */
    grid-template-columns: 1fr 1fr; /* 2 contacts per row */
    gap: 16px; /* Space between contacts */
    font-family: "Outfit", sans-serif !important;
}

.popup-contact .contact-column { 
    display: flex;
    flex-direction: column; 
    gap: 8px; /* Add space between contact details */
}

/* Contact Name */
.contact-name { 
    font-weight: bold; 
    font-family: "Outfit", sans-serif !important;
}

/* Contact Details */
.contact-details { 
    display: flex; 
    align-items: center; 
    gap: 8px;
    font-family: "Outfit", sans-serif !important;
}
        /* Icons */
        .icon { 
            width: 14px; 
            height: 14px; 
        }
  /* Actions: Directions & More Info */
  .popup-actions { 
            display: flex; 
            background: #fff; 
            border-top: 1px solid #ccc; 
            text-align: center; 
        }

        .popup-actions a { 
            flex: 1; 
            font-size: 12px; 
            cursor: pointer; 
            padding: 10px;
            text-decoration: none;
            color: #333;
            display: block;
            font-family: "Outfit", sans-serif !important;
        }

        .popup-actions a{ 
            border-right: 1px solid #ccc; 
            
        }
        .popup-actions a:last-child { 
            border-right: none; 
        }

         /* Custom Leased Icon with label below */
         .custom-retail-icons {
            position: relative;
            width: 22px !important;
            height: 22px !important;
            background-color: #c4ad81; /* Orange background */
            color: white;
            font-size: 12px;
            font-weight: bold;
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center;
            font-family: "Outfit", Sans-serif !important;
            border-radius: 2px 2px 0 0;
            font-family: "Outfit", sans-serif !important;
        }

        .custom-retail-icons::after {
            content: '';
            position: absolute;
            bottom: -6px; /* Position the triangle */
            left: 50%;
            transform: translateX(-50%);
            width: 0;
            height: 0;
            border-left: 6px solid transparent;
            border-right: 6px solid transparent;
            border-top: 8px solid #c4ad81; /* Orange triangle */
        }
        .custom-pad-icons {
            position: relative;
            width: 22px !important;
            height: 22px !important;
            background-color: #023C5B; /* Orange background */
            color: white;
            font-size: 12px;
            font-weight: bold;
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center;
            font-family: "Outfit", Sans-serif !important;
            border-radius:2px;
            font-family: "Outfit", sans-serif !important;
        }

        .custom-pad-icons::after {
            content: '';
            position: absolute;
            bottom: -6px; /* Position the triangle */
            left: 50%;
            transform: translateX(-50%);
            width: 0;
            height: 0;
            border-left: 6px solid transparent;
            border-right: 6px solid transparent;
            border-top: 8px solid #023C5B; /* Orange triangle */
        }
   /* Filter Control */
        .filter-control {
            position: absolute;
            top: 10px;
            right: 10px;
            z-index: 1000;
            background: white;
            padding: 15px;
            border-radius: 5px;
            box-shadow: 0 0 10px rgba(0,0,0,0.2);
            font-family: 'Outfit', sans-serif;
            width: 220px;
        }
        
        .filter-control h3 {
            margin: 0 0 10px 0;
            font-size: 14px;
        }
        
        .filter-select {
            width: 100%;
            padding: 8px;
            border: 1px solid #ddd;
            border-radius: 4px;
            font-family: 'Outfit', sans-serif;
            font-size: 13px;
            background-color: #fff;
            cursor: pointer;
            margin-bottom: 10px;
        }
        
        .filter-select:last-child {
            margin-bottom: 0;
        }
        
        .filter-select:focus {
            outline: none;
            border-color: #023C5B;
        }
        
